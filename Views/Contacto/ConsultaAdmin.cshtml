@model IEnumerable<myapp.Models.Contactos>
@{
    ViewData["Title"] = "Contacto";  
}
<link rel="stylesheet" href="~/css/Carrito.css" asp-append-version="true" />
 <link rel="stylesheet" href="~/css/Footer.css" asp-append-version="true" />
  <link rel="stylesheet" href="~/css/Tablas.css" asp-append-version="true" />
<h1>Consultas</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Nombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CorreoElectronico)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Consulta)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CorreoElectronico)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Consulta)
            </td>
             <td>
                <a asp-action="Details" asp-route-id="@item.Id" class="control">Details</a> |
            </td>
        </tr>
}
    </tbody>
</table>

